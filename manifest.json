{
    "groups": [
        {
            "id": "resolution",
            "name": "Resolution",
            "isExpanded": true
        },
        {
            "id": "dynamic-range",
            "name": "Dynamic Range",
            "isExpanded": false
        },
        {
            "id": "media-source",
            "name": "Media Source",
            "isExpanded": false
        },
        {
            "id": "audio",
            "name": "Audio",
            "isExpanded": false
        }
    ],
    "filters": [
        {
            "id": "2160p",
            "name": "4K",
            "pattern": "2160p|4k|uhd|ultra.?hd|3840\\s?x\\s?\\d{4}",
            "groupId": "resolution",
            "enabled": true,
            "style": "filled and bordered",
            "tagColor": "#000000",
            "textColor": "#ffffff",
            "imageUrl": "https://raw.githubusercontent.com/jmxd/Kometa/refs/heads/main/overlays/images/media_info/resolution/Ultra-HD.png"
        },
        {
            "id": "1080p",
            "name": "1080p",
            "pattern": "1080p|full.?hd|fhd|hd1080|1920\\s?x\\s?\\d{3,4}",
            "groupId": "resolution",
            "enabled": true,
            "style": "filled and bordered",
            "tagColor": "#000000",
            "textColor": "#ffffff",
            "imageUrl": ""
        },
        {
            "id": "720p",
            "name": "720p",
            "pattern": "720p|hd720|1280\\s?x\\s?\\d{3,4}",
            "groupId": "resolution",
            "enabled": true,
            "style": "filled and bordered",
            "tagColor": "#000000",
            "textColor": "#000000",
            "imageUrl": "https://raw.githubusercontent.com/jmxd/Kometa/refs/heads/main/overlays/images/media_info/resolution/1080P.png"
        },
        {
            "id": "hdr",
            "name": "HDR",
            "pattern": "hdr(?!10|10\\+|plus)",
            "groupId": "dynamic-range",
            "enabled": true,
            "style": "filled and bordered",
            "tagColor": "#000000",
            "textColor": "#000000",
            "imageUrl": "https://raw.githubusercontent.com/jmxd/Kometa/refs/heads/main/overlays/images/media_info/codec/HDR.png"
        },
        {
            "id": "hdr10",
            "name": "HDR10",
            "pattern": "hdr10(?!\\+|plus)",
            "groupId": "dynamic-range",
            "enabled": true,
            "style": "filled and bordered",
            "tagColor": "#000000",
            "textColor": "#000000",
            "imageUrl": ""
        },
        {
            "id": "hdr10+",
            "name": "HDR10+",
            "pattern": "hdr10[ .\\-_]?(plus|\\+)",
            "groupId": "dynamic-range",
            "enabled": true,
            "style": "filled and bordered",
            "tagColor": "#000000",
            "textColor": "#000000",
            "imageUrl": "https://raw.githubusercontent.com/jmxd/Kometa/refs/heads/main/overlays/images/media_info/codec/Plus.png"
        },
        {
            "id": "dolby-vision",
            "name": "Dolby Vision",
            "pattern": "dv|dovi|dolby[ .]?v(ision)?",
            "groupId": "dynamic-range",
            "enabled": true,
            "style": "filled and bordered",
            "tagColor": "#000000",
            "textColor": "#ffffff",
            "imageUrl": "https://raw.githubusercontent.com/jmxd/Kometa/refs/heads/main/overlays/images/media_info/codec/DV.png"
        },
        {
            "id": "sdr",
            "name": "SDR",
            "pattern": "sdr",
            "groupId": "dynamic-range",
            "enabled": true,
            "style": "filled and bordered",
            "tagColor": "#000000",
            "textColor": "#ffffff",
            "imageUrl": ""
        },
        {
            "id": "hdr-undefined",
            "name": "HDR (Undefined)",
            "pattern": "(?=.*\\b(2160p)\\b)(?=.*\\b(FraMeSToR|HQMUX|SiCFoI)\\b)(?!.*\\b(DV|DoVi|Dolby[ .]?V(ision)?|HDR10(?!\\+|Plus)|HDR10\\+|HDR10Plus|HLG|PQ|SDR(?!\\d))\\b)",
            "groupId": "dynamic-range",
            "enabled": true,
            "style": "filled and bordered",
            "tagColor": "#000000",
            "textColor": "#000000",
            "imageUrl": "https://raw.githubusercontent.com/jmxd/Kometa/refs/heads/main/overlays/images/media_info/codec/HDR.png"
        },
        {
            "id": "remux",
            "name": "REMUX",
            "pattern": "remux",
            "groupId": "media-source",
            "enabled": true,
            "style": "filled and bordered",
            "tagColor": "#000000",
            "textColor": "#ffffff",
            "imageUrl": ""
        },
        {
            "id": "blu-ray",
            "name": "Blu-ray",
            "pattern": "blu[-_]?ray",
            "groupId": "media-source",
            "enabled": true,
            "style": "filled and bordered",
            "tagColor": "#000000",
            "textColor": "#ffffff",
            "imageUrl": ""
        },
        {
            "id": "web",
            "name": "WEB",
            "pattern": "web[-_.]?(dl|rip)",
            "groupId": "media-source",
            "enabled": true,
            "style": "filled and bordered",
            "tagColor": "#000000",
            "textColor": "#ffffff",
            "imageUrl": ""
        },
        {
            "id": "remaster",
            "name": "Remaster",
            "pattern": "remaster",
            "groupId": "media-source",
            "enabled": true,
            "style": "filled and bordered",
            "tagColor": "#000000",
            "textColor": "#ffffff",
            "imageUrl": ""
        },
        {
            "id": "atmos",
            "name": "Dolby Atmos",
            "pattern": "atmos",
            "groupId": "audio",
            "enabled": true,
            "style": "filled and bordered",
            "tagColor": "#000000",
            "textColor": "#ffffff",
            "imageUrl": "https://raw.githubusercontent.com/jmxd/Kometa/refs/heads/main/overlays/images/media_info/codec/Atmos.png"
        },
        {
            "id": "dts-hd-ma",
            "name": "DTS-HD MA",
            "pattern": "dts[-_. ]?(ma|hd([-_. ]?ma)?|xll)",
            "groupId": "audio",
            "enabled": true,
            "style": "filled and bordered",
            "tagColor": "#000000",
            "textColor": "#ffffff",
            "imageUrl": ""
        },
        {
            "id": "dts-hd",
            "name": "DTS-HD",
            "pattern": "dts[ .\\-_]?hd(?!ma)",
            "groupId": "audio",
            "enabled": true,
            "style": "filled and bordered",
            "tagColor": "#000000",
            "textColor": "#ffffff",
            "imageUrl": "https://raw.githubusercontent.com/jmxd/Kometa/refs/heads/main/overlays/images/media_info/codec/DTS-HD.png"
        },
        {
            "id": "truehd",
            "name": "TrueHD",
            "pattern": "true[ .-]?hd",
            "groupId": "audio",
            "enabled": true,
            "style": "filled and bordered",
            "tagColor": "#000000",
            "textColor": "#ffffff",
            "imageUrl": "https://raw.githubusercontent.com/jmxd/Kometa/refs/heads/main/overlays/images/media_info/codec/TrueHD.png"
        },
        {
            "id": "truehd-atmos",
            "name": "TrueHD Atmos",
            "pattern": "(?=.*\\b(True[ .-]?HD|W4NK3R|HQMUX)\\b)(?=.*\\b(ATMOS|CtrlHD|W4NK3R|DON)\\b)",
            "groupId": "audio",
            "enabled": true,
            "style": "filled and bordered",
            "tagColor": "#000000",
            "textColor": "#ffffff",
            "imageUrl": "https://raw.githubusercontent.com/jmxd/Kometa/refs/heads/main/overlays/images/media_info/codec/TrueHD-Atmos.png"
        },
        {
            "id": "dts-x",
            "name": "DTS-X",
            "pattern": "dts[-_.: ]?x7?",
            "groupId": "audio",
            "enabled": true,
            "style": "filled and bordered",
            "tagColor": "#000000",
            "textColor": "#ffffff",
            "imageUrl": "https://raw.githubusercontent.com/jorgitoxp/MediaTags/refs/heads/main/overlays/images/media_info/codec/DTS-X.png"
        },
        {
            "id": "dd+",
            "name": "Dolby Digital+",
            "pattern": "DD[P+]|e[-_. ]?ac-?3",
            "groupId": "audio",
            "enabled": true,
            "style": "filled and bordered",
            "tagColor": "#000000",
            "textColor": "#ffffff",
            "imageUrl": "https://raw.githubusercontent.com/jmxd/Kometa/refs/heads/main/overlays/images/media_info/codec/DigitalPlus.png"
        }
    ]
}
